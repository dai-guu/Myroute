<%= form_with(model: @map) do |f| %>
 <div class="field my-3">
   <%= f.label :title %>
   <%= f.text_field :title %>
 </div>
 <div class="field my-3">
   <%= f.label :description %>
   <%= f.text_area :description %>
 </div>
 <div class="field my-3">
   <%= f.label :address %>
   <%= f.text_field :address %>
 </div>
 <div class="actions">
   <%= f.submit %>
 </div>
<% end %>